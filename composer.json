{
    "name": "owenmelbz/laravel-stubs",
    "description": "Easily store stubs for your project to reuse whenever needed.",
    "keywords": ["laravel", "stubs", "templates"],
    "license": "MIT",
    "authors": [
        {
            "name": "Owen Melbourne",
            "email": "owenmelbz@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "OwenMelbz\\LaravelStubs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwenMelbz\\LaravelStubs\\StubsServiceProvider"
            ]
        }
    }
}
